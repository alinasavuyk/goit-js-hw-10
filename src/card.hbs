{{#each this}}
<div class="card">
<div class="card-img-top">
<img src={{flags.svg}} alt={{name}} width="30">
</div>
<div class="card-body">
<h2 class="card-title">{{name.common}}</h2>
<p class="card-text">Capital: {{capital}}</p>
<p class="card-text">Population: {{population}}</p>
<p class="card-text">Languages:{{#each languages}}<span class="language">{{this}}</span>{{/each}}</p>
</div>
</div>
{{/each}}
